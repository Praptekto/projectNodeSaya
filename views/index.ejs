<html lang="en">
<%- include('./partial/head.ejs')   %>
<body>
<%- include('./partial/nav.ejs')  %>

    <div class="blogs content">
        <h3>All Blogs</h3>
        <% if(blogs.length>0){ %>
          <%  blogs.forEach(blog=>{ %>

            <a class ="single" href="/blog/<%=blog.id %>">
              <h1>  <%= blog.title %></h1>
              <p> <%= blog.snippet %></p>
              <p> <%= blog.body %></p>
            <% })} %>
            </a>
    </div>
</body>

<%- include('./partial/footer.ejs')  %>

</html>
